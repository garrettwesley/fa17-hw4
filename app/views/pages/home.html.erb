<h1> Cats </h1>
    <% @cats.each do |cat| %>
    <%= cat.name %>
    <% end %>

<h1> Users </h1>
    <% @users.each do |user| %>
    <%= user.name %>
    <%= user.age %>
    <%= user.email %>
    <% end %>

<%= link_to 'Click Here to create a new TODO', todos_new_path %>

<h1> Todos </h1>
    <% @todos.each do |todo| %>
    <%= todo.tasks %>
    <%= todo.finished %>
    <% end %>